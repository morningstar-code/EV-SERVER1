{
  "compilerOptions": {
    "target": "ES2020",
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "types": ["@citizenfx/client", "@types/node", "webpack-env"],
    "lib": ["ES2020"]
  },
  "include": ["./**/*"],
  "exclude": ["**/node_modules", "**/__tests__/*"]
}